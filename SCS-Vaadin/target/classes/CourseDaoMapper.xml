<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="SCS.CourseDao">
 
  <select id="select_course" resultType="SCS.Course">
    select * from course where course_id = #{0}
  </select>
  
  <select id="select_all" resultType="SCS.Course">
    select * from course
  </select>
  
</mapper>
