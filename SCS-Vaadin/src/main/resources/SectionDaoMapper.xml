<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="SCS.SectionDao">
  
  <select id="get_section" parameterType="String" resultType="SCS.Section">
    select * from section where course_id = #{0}
  </select>
  
  
